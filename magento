upstream fastcgi_backend {
        server  unix:/run/php/php7.2-fpm.sock;
}

server {
    listen 80;
    listen [::]:80;
    server_name peliyhteiso.net;
    return 301 https://$server_name$request_uri;
}

server {

        listen 443 ssl;
        server_name peliyhteiso.net;

        ssl on;
        ssl_certificate /etc/letsencrypt/live/peliyhteiso.net/fullchain.pem;
        ssl_certificate_key /etc/letsencrypt/live/peliyhteiso.net/privkey.pem;

        set $MAGE_ROOT /var/www/magento2;
        set $MAGE_MODE developer;
        include /var/www/magento2/nginx.conf.sample;
